{
  "name": "@ebarooni/eslint-config",
  "version": "0.0.2",
  "description": "The standard shareable config for ESLint",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./ts": "./typescript-esling-config.js",
    "./angular": "./angular-eslint-config.js"
  },
  "files": [
    "index.js",
    "typescript-esling-config.js",
    "angular-eslint-config.js",
    "README.md",
    "LICENSE.txt"
