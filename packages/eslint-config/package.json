{
  "name": "@ebarooni/eslint-config",
  "version": "1.2.0",
  "description": "A shareable ESLint configuration for JavaScript, TypeScript, and Angular projects",
  "license": "MIT",
  "author": "Ehsan Barooni",
  "homepage": "https://github.com/ebarooni/dev-tools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebarooni/dev-tools.git"
  },
  "bugs": {
    "url": "https://github.com/ebarooni/dev-tools/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "linting",
    "typescript",
    "angular"
  ],
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "files": [
    "index.js",
    "index.mjs",
    "configs/**"
  ],
  "peerDependencies": {
    "@eslint/js": "^9.19.0",
    "@eslint/json": "^0.10.0",
    "@eslint/markdown": "^6.2.2",
    "angular-eslint": "^18.0.0 || ^19.0.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-promise": "^7.2.1",
    "typescript": "*",
    "typescript-eslint": "^8.22.0"
  },
  "peerDependenciesMeta": {
    "@eslint/json": {
      "optional": true
    },
    "@eslint/markdown": {
      "optional": true
    },
    "angular-eslint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.12.2"
  },
  "scripts": {
    "prepublishOnly": "cp ../../LICENSE . || echo 'License file not found, skipping copy'",
    "postpublish": "rm -f LICENSE"
  }
}
